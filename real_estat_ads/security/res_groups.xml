<odoo>
    <record id="module_category_real_estat" model="ir.module.category">
        <field name="name">Real Estat</field>
        <field name="description">Helps you handle properties, and offers</field>
        <field name="sequence">50</field>
    </record>

    <record id="group_property_user" model="res.groups">
        <field name="name">User: Manage own property</field>
        <field name="category_id" ref="real_estat_ads.module_category_real_estat"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[Command.link(ref('base.user_demo'))]"/>
    </record>

    <record id="group_property_manager" model="res.groups">
        <field name="name">Manager: Manage all property</field>
        <field name="category_id" ref="real_estat_ads.module_category_real_estat"/>
        <field name="implied_ids" eval="[(4, ref('real_estat_ads.group_property_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_admin'))]"/>
    </record>

    <record id="group_property_test" model="res.groups">
        <field name="name">Test: Test Manage property</field>
        <field name="category_id" ref="base.module_category_hidden"/>
    </record>
</odoo>
